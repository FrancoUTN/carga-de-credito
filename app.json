{
  "expo": {
    "name": "carga-de-credito",
    "slug": "carga-de-credito",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
